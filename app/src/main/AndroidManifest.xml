<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fsoon.android.fsfirsttemplate">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FsFirstTemplate"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".view.tamplate.CollapsHeaderActivity"
            android:exported="false" />
        <activity
            android:name=".view.tamplate.ScrollingHeaderActivity"
            android:exported="false" />
        <activity
            android:name=".view.tamplate.FixedHeaderActivity"
            android:exported="false" />
        <activity
            android:name=".view.test.TestNetworkActivity"
            android:exported="false" />
        <activity
            android:name=".view.test.TestHeaderActivity"
            android:exported="false" />
        <activity
            android:name=".view.test.TestActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>